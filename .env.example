# Remote Database
DBMIGRATE_REMOTE_HOST=your-remote-host.com
DBMIGRATE_REMOTE_PORT=5432
DBMIGRATE_REMOTE_DATABASE=production_db
DBMIGRATE_REMOTE_USER=readonly_user
DBMIGRATE_REMOTE_PASSWORD=your-password
DBMIGRATE_REMOTE_SSLMODE=require

# Local Database
DBMIGRATE_LOCAL_HOST=localhost
DBMIGRATE_LOCAL_PORT=5433
DBMIGRATE_LOCAL_DATABASE=local_db
DBMIGRATE_LOCAL_USER=postgres
DBMIGRATE_LOCAL_PASSWORD=postgres
DBMIGRATE_LOCAL_SSLMODE=disable

# Docker
DBMIGRATE_DOCKER_CONTAINER_NAME=postgres-local
DBMIGRATE_DOCKER_COMPOSE_FILE=docker-compose.yml
DBMIGRATE_DOCKER_AUTO_START=true

# Migration
DBMIGRATE_MIGRATION_ANONYMIZE=true
DBMIGRATE_MIGRATION_TRUNCATE_TABLES=true
DBMIGRATE_MIGRATION_BATCH_SIZE=1000

# Logging
DBMIGRATE_LOGGING_LEVEL=info
DBMIGRATE_LOGGING_OUTPUT_PATH=stdout
DBMIGRATE_LOGGING_FORMAT=console
